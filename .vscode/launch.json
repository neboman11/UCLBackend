            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Frontend",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/UCLFrontend/bin/Debug/net5.0/UCLFrontend.dll",
            "args": [],

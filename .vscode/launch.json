            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build Discord",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/UCLBackend.Discord/bin/Debug/net5.0/UCLBackend.Discord.dll",
            "args": [],
            "cwd": "${workspaceFolder}/UCLBackend.Discord",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false,
            "envFile": "${workspaceFolder}/UCLBackend.Discord/.env"
